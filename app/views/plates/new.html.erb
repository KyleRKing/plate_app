<h1>RoaD RageR: Enter State Code & Plate #</h1>

<%= form_for @plate do |form| %>
	<%= form.text_field :plate_state, :placeholder => "State Code" %></br>
	<%= form.text_field :plate_number, :placeholder => "License Plate ID" %></br>
	<%=  form.text_field :user, :placeholder => "Enter temp ID" %></br></br>

	<%= form.submit %></br></br>
<% end %>


